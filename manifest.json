{
  "manifest_version": 3,
  "name": "My skills",
  "description": "My skills chrome extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "public/icon-16.png",
    "32": "public/icon-32.png",
    "48": "public/icon-48.png",
    "100": "public/icon-100.png"
  },
  "permissions": ["activeTab"]
}